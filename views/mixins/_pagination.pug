mixin pagination(page, pages, count)
	.pagination
		.pagination__prev
			if page > 1
				a(href=`/stores/page/${page - 1}`) Forrige
		.pagination__text
			p Side #{page} av #{pages} - #{count} totale steder
		.pagination__next
			if page < pages
				a(href=`/stores/page/${parseFloat(page) + 1}`) Neste 